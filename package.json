    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nurhamsah1998/language-center-technical-test.git"
  },
  "bugs": {
    "url": "https://github.com/nurhamsah1998/language-center-technical-test/issues"
  },
  "homepage": "https://github.com/nurhamsah1998/language-center-technical-test#readme",
  "dependencies": {
    "concurrently": "^9.2.0"
  }
}
